            <move id="bypass" icon="BeachRaceBypassMoveIcon@publicBeach" label="beachRaceBypassLabel" count="2">
                <points>
                    <type id="bonus" shift="0" points="5"/>
                    <type id="barrier" shift="1" points="25"/>
                    <type id="portal" shift="0" points="5"/>
                </points>
